from building import *
import rtconfig

# get current directory
cwd     = GetCurrentDir()
# The set of source files associated with this SConscript file.
src     = Glob('*.c')

path    = [cwd + '/']

LOCAL_CCFLAGS = ''

group = DefineGroup('whetstone', src, depend = ['LPKG_USING_WHETSTONE'], CPPPATH = path, LOCAL_CCFLAGS = LOCAL_CCFLAGS)

Return('group')
