
# Kconfig file for package aclock
menuconfig PKG_USING_ACLOCK
    bool "aclock: a terminal clock"
    default n

if PKG_USING_ACLOCK

    config PKG_ACLOCK_PATH
        string
        default "/packages/misc/entertainment/aclock"

    config PKG_ACLOCK_VER
       string
       default "latest"

endif

